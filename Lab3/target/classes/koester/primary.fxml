<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="350.0" prefWidth="327.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="koester.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label text="City Information">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Pane prefHeight="200.0" prefWidth="200.0">
         <children>
            <ComboBox fx:id="cityCombo" layoutX="144.0" layoutY="98.0" onAction="#setData" prefHeight="25.0" prefWidth="123.0" promptText="Select City" />
            <ComboBox fx:id="stateCombo" layoutX="13.0" layoutY="98.0" onAction="#setCities" prefHeight="25.0" prefWidth="109.0" promptText="Select State" />
            <Button layoutX="198.0" layoutY="14.0" mnemonicParsing="false" onAction="#setStates" text="Read" />
            <TextField fx:id="userInput" layoutX="14.0" layoutY="14.0" />
            <Label layoutX="42.0" layoutY="45.0" text="Input File" />
            <Label layoutX="22.0" layoutY="139.0" text="Zipcode:" />
            <Label layoutX="22.0" layoutY="156.0" text="Time Zone:" />
            <Label fx:id="readLbl" layoutX="20.0" layoutY="70.0" />
            <Label fx:id="zipLbl" layoutX="193.0" layoutY="139.0" prefHeight="17.0" prefWidth="59.0" />
            <Label fx:id="tzLbl" layoutX="191.0" layoutY="156.0" />
         </children>
      </Pane>
   </children>
</VBox>
