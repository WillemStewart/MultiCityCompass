<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="350.0" prefWidth="327.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="koester.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label text="City Information" />
      <Pane prefHeight="200.0" prefWidth="200.0">
         <children>
            <ComboBox fx:id="cityCombo" layoutX="137.0" layoutY="75.0" onAction="#citySelected" prefWidth="150.0" promptText="Select City" />
            <Button layoutX="198.0" layoutY="14.0" mnemonicParsing="false" onAction="#readCities" text="Read" />
            <TextField fx:id="userInput" layoutX="14.0" layoutY="14.0" />
            <Label layoutX="42.0" layoutY="58.0" text="Input File" />
            <Label layoutX="22.0" layoutY="122.0" text="State:" />
            <Label layoutX="22.0" layoutY="139.0" text="Zipcode:" />
            <Label layoutX="22.0" layoutY="156.0" text="Time Zone:" />
            <Label fx:id="stateLbl" layoutX="200.0" layoutY="122.0" />
            <Label fx:id="zipLbl" layoutX="200.0" layoutY="139.0" />
            <Label fx:id="tzLbl" layoutX="201.0" layoutY="156.0" />
            <Label fx:id="readLbl" layoutX="11.0" layoutY="79.0" prefHeight="17.0" prefWidth="122.0" textFill="#11f209">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</VBox>
